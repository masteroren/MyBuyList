/*Reorder List*/
.dragHandle {
	width:10px;
	height:15px;
	background-color:Red;
	cursor:move;
	border:outset thin white;
}

.callbackStyle {
	border:thin blue inset;		
}

.callbackStyle table {
	background-color:#5377A9;	
	color:Black;
}


.reorderList li {
	list-style:none;
	margin:1px;
}

.reorderCue {
	border:dashed thin black;
	width:100%;
	height:25px;
}

.itemArea {
	margin-left:10px;
	font-family:Arial, Verdana, sans-serif;
	font-size:1em;
	text-align:left;
}

ul
{
	margin:0px 5px 0px 5px;
}